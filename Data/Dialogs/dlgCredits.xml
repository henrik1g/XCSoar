<?xml version="1.0"?>

<Form Caption="Configuration">
  <SymbolButton Name="cmdNext" Caption="&gt;" X="60" Y="-35" Width="60" Height="35" OnClick="OnNext" />
  <Button Name="cmdClose" Caption="Close" X="-120" Y="-35" Height="35" OnClick="OnClose" />

  <Tabbed Name="tab" X="0" Y="0" Height="-35">
    <Panel Name="frmInfo">
      <Canvas OnPaint="OnLogoPaint" />
    </Panel>
  
    <Panel>
      <TextEdit Name="Changelog" ReadOnly="1" MultiLine="1" />
    </Panel>

    <Panel Name="frmAuthors">
      <TextEdit Name="prpAuthors" ReadOnly="1" MultiLine="1" />
    </Panel>
  
    <Panel Name="frmLicense">
      <TextEdit Name="prpLicense" ReadOnly="1" MultiLine="1" />
    </Panel>
  </Tabbed>

  <SymbolButton Name="cmdPrev" Caption="&lt;" X="0" Y="-35" Width="60" Height="35" OnClick="OnPrev" />
</Form>
